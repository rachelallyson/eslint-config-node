{
    "name": "@rachelallyson/eslint-config-node",
    "version": "1.0.1",
    "description": "Modular ESLint configuration for Node.js projects - pick and choose the configs you need",
    "main": "index.js",
    "type": "module",
    "exports": {
        ".": "./index.js",
        "./base": "./configs/base.js",
        "./graphql": "./configs/graphql.js",
        "./import": "./configs/import.js",
        "./prettier": "./configs/prettier.js",
        "./security": "./configs/security.js",
        "./ts": "./configs/ts.js",
        "./tsconfig.json": "./tsconfig.json"
    },
    "files": [
        "index.js",
        "configs",
        "tsconfig.json"
    ],
    "keywords": [
        "eslint",
        "eslint-config",
        "node",
        "nodejs",
        "typescript",
        "graphql",
        "prettier"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rachelallyson/eslint-config-node.git"
    },
    "bugs": {
        "url": "https://github.com/rachelallyson/eslint-config-node/issues"
    },
    "homepage": "https://github.com/rachelallyson/eslint-config-node#readme",
    "scripts": {
        "prepublishOnly": "./scripts/ensure-git-tag.sh",
        "postversion": "git push && git push --tags",
        "docs:dev": "cd docs && next dev",
        "docs:build": "cd docs && next build",
        "docs:start": "cd docs && next start",
        "docs:api": "typedoc"
    },
    "peerDependencies": {
        "eslint": ">= 9",
        "typescript": ">=4.8.4 <6.0.0"
    },
    "peerDependenciesMeta": {
        "typescript": {
            "optional": true
        }
    },
    "author": "Kuzu Media",
    "license": "ISC",
    "dependencies": {
        "@eslint/compat": "^1.4.1",
        "@eslint/js": "^9.38.0",
        "@graphql-eslint/eslint-plugin": "^4.4.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-n": "^17.23.1",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-sort-destructure-keys": "^2.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-unused-imports": "^4.3.0",
        "prettier": "^3.6.2",
        "typescript-eslint": "^8.46.2"
    },
    "devDependencies": {
        "typedoc": "^0.28.14",
        "typedoc-plugin-markdown": "^4.9.0"
    }
}