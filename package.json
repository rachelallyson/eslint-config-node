{
    "name": "@rachelallyson/eslint-config-node",
    "version": "1.0.0",
    "description": "Modular ESLint configuration for Node.js projects - pick and choose the configs you need",
    "main": "index.js",
    "type": "module",
    "exports": {
        ".": "./index.js",
        "./base": "./configs/base.js",
        "./graphql": "./configs/graphql.js",
        "./import": "./configs/import.js",
        "./prettier": "./configs/prettier.js",
        "./security": "./configs/security.js",
        "./ts": "./configs/ts.js"
    },
    "files": [
        "index.js",
        "configs",
        "tsconfig.json"
    ],
    "keywords": [
        "eslint",
        "eslint-config",
        "node",
        "nodejs",
        "typescript",
        "graphql",
        "prettier"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rachelallyson/eslint-config-node.git"
    },
    "bugs": {
        "url": "https://github.com/rachelallyson/eslint-config-node/issues"
    },
    "homepage": "https://github.com/rachelallyson/eslint-config-node#readme",
    "scripts": {
        "prepublishOnly": "./scripts/ensure-git-tag.sh",
        "postversion": "git push && git push --tags"
    },
    "peerDependencies": {
        "eslint": ">= 9",
        "typescript": ">=4.8.4 <6.0.0"
    },
    "peerDependenciesMeta": {
        "typescript": {
            "optional": true
        }
    },
    "author": "Kuzu Media",
    "license": "ISC",
    "dependencies": {
        "@eslint/compat": "^1.2.4",
        "@eslint/js": "^9.17.0",
        "@graphql-eslint/eslint-plugin": "^4.3.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-n": "^17.15.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-sort-destructure-keys": "^2.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-unused-imports": "^4.1.4",
        "prettier": "^3.4.2",
        "typescript-eslint": "^8.18.1"
    }
}